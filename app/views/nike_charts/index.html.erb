<h1>NikePlus Charts <%= @listActivitiesJson %> </h1>

<script type="text/javascript" charset="utf-8">
	$(function() {
		new Highcharts.Chart({
			chart: {
				renderTo: "chart1"
			},
			series: [{
				data: <%= @distances %>
			}],
			credits: [{
				enabled: false
			}]
		});
	});
</script>

<p> Test Chart <%= @data %> </p>
<div id="chart1" style="width: 560px; height: 300px;"> </div>
<p> Request: <%= @request_uri_str %> </p>
<p> Result: <%= @distances %> </p>

